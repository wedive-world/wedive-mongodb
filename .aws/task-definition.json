{
    "family": "mongo-express",
    "executionRoleArn": "arn:aws:iam::681728264552:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "mongo-express",
            "image": "public.ecr.aws/t3p3g5h0/wedive-public-container-image-repo/mongo-express:latest",
            "portMappings": [
                {
                    "containerPort": 8081,
                    "hostPort": 8081,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}