{
    "family": "mongo-express",
    "executionRoleArn": "arn:aws:iam::681728264552:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "mongo-express",
            "image": "681728264552.dkr.ecr.ap-northeast-2.amazonaws.com/mongo-express:latest",
            "portMappings": [
                {
                    "containerPort": 8081,
                    "hostPort": 4200,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}